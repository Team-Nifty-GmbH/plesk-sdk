{
    "name": "team-nifty-gmbh/plesk-sdk",
    "description": "PHP SDK for Plesk RESTful API",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Team Nifty GmbH",
            "email": "info@team-nifty.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "saloonphp/saloon": "^3.0",
        "spatie/laravel-data": "^4.0",
        "illuminate/support": "^11.0|^12.0",
        "illuminate/contracts": "^11.0|^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "TeamNifty\\Plesk\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "TeamNifty\\Plesk\\PleskServiceProvider"
            ],
            "aliases": {
                "Plesk": "TeamNifty\\Plesk\\Facades\\Plesk"
            }
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TeamNifty\\Plesk\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
